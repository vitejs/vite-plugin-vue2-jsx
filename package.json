{
  "name": "@vitejs/plugin-vue2-jsx",
  "version": "1.0.3",
  "license": "MIT",
  "author": "Evan You",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild && pnpm run patch-cjs",
    "patch-cjs": "tsx ./scripts/patchCJS.ts",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitejs/plugin-vue2-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/vitejs/plugin-vue2-jsx/issues"
  },
  "homepage": "https://github.com/vitejs/plugin-vue2-jsx#readme",
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.18.6",
    "@rollup/pluginutils": "^5.0.2",
    "@vue/babel-preset-jsx": "^1.3.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "picocolors": "^1.0.0",
    "tsx": "^3.7.1",
    "unbuild": "^0.7.4",
    "vite": "^4.0.0",
    "vue": "^2.7.4"
  },
  "peerDependencies": {
    "vite": "^2.9.13 || ^3.0.0 || ^4.0.0",
    "vue": "^2.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
